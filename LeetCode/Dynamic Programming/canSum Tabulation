
int canSum(int targetSum , int*numbers , int numberSize){
    
int dp[targetSum + 1];

for(int i = 0 ; i<=targetSum  ; i++){
    dp[i]  = 0;
}

dp[0] = 1;


for(int i =0 ; i <= targetSum ; i++){
    
    for(int j = 0 ; j<  numberSize  ; j++){
        
        if(i + numbers[j] <= targetSum){
            dp[ i + numbers[j] ] = 1;
        }
    }
    
    
}


    return dp[targetSum];
}
